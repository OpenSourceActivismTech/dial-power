    "debug": "~2.6.3",
    "ejs": "^2.5.6",
    "express": "~4.15.2",
    "knex": "^0.13.0",
    "morgan": "~1.8.1",
    "pg": "^6.4.1",
    "pug": "~2.0.0-beta11",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-form": "^7.0.1",
